@model EntityLayer.Concrete.Service
@{
    ViewData["Title"] = "EditService";
    Layout = "~/Views/Shared/_NewAdminLayout.cshtml";
}


 <div class="col-12 grid-margin stretch-card">
                <div class="card">
                  <div class="card-body">
                    <h4 class="card-title">Deneyim Ekle</h4>
                  
@using(Html.BeginForm("EditService","Service",FormMethod.Post))
{

    @Html.HiddenFor(x=>x.ServiceID);
    <div>
        <label>Deneyim Adı</label>
        @Html.TextBoxFor(x=>x.Title, new {@class="form-control"})
        <br>
    </div>

    <div>
        <label>Deneyim Açıklaması</label>
        @Html.TextBoxFor(x=>x.ImageUrl, new {@class="form-control"})
        <br>
    </div>


  
    <button class="btn btn-success">Ekle</button>
}



                  </div>
                </div>
              </div>




